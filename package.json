{
  "name": "cahoots-brewery",
  "version": "0.1.0",
  "description": "Website for Cahoots Brewery",
  "main": "index.js",
  "author": "Caleb Sanchez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/calebmichaelsanchez/cahoots-brewery/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calebmichaelsanchez/cahoots-brewery.git"
  },
  "homepage": "https://github.com/calebmichaelsanchez/cahoots-brewery#readme",
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "chokidar-cli": "^1.0.1",
    "eslint": "^1.5.1",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^2.1.0",
    "webpack": "^1.12.2"
  },
  "config": {
    "css_file": "./sqs_template/styles/app.css",
    "scss_file": "./scss/app.scss"
  },
  "scripts": {
    "lint:js": "eslint js",
    "build": "npm run build:js && npm run build:css",
    "build:js": "webpack -d",
    "build:css": "sass --sourcemap=none --scss $npm_package_config_scss_file $npm_package_config_css_file",
    "postbuild:css": "postcss --use autoprefixer --autoprefixer.browsers 'last 2 versions' --output $npm_package_config_css_file $npm_package_config_css_file",
    "watch": "parallelshell 'npm run watch:js' 'npm run watch:sass'",
    "watch:js": "chokidar ./js/* --initial -d 10 -c 'npm run build:js'",
    "watch:sass": "chokidar ./scss/* --initial -d 10 -c 'npm run build:css'"
  },
  "dependencies": {
    "axios": "^0.8.1",
    "bootstrap": "^3.3.5",
    "jquery": "^2.1.4",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-slick": "^0.9.3"
  }
}
